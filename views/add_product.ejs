    <div class="container mt-5">
      <h2>Add Product</h2>
      <form action="/addProductData" method="post" >
        <div class="mb-3">
          <label for="productName" class="form-label">Product Name</label>
          <input type="text" class="form-control" id="productName" name="product_name" required>
        </div>
        <div class="mb-3">
          <label for="price" class="form-label">Price</label>
          <input type="number" class="form-control" id="price" name="price" required>
        </div>
        <div class="mb-3">
          <label for="link" class="form-label">image Link</label>
          <input type="url" class="form-control" id="link" name="product_image_url" required value="https://lh3.googleusercontent.com/drive-viewer/AEYmBYR-J3gPcHNXfH0Cfypup4RlnizEmM9vdsXQq8jrBENoWACpPD5cFyFZE6acHFopwrZNp8mG3S38CtZJiSviJhTVYpQf=s1600" >
